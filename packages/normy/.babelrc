{
  "presets": [
    "@babel/preset-typescript",
    ["@babel/preset-env", { "loose": true, "modules": false }]
  ],
  "env": {
    "cjs": {
      "presets": [["@babel/preset-env", { "loose": true }]]
    }
  },
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true
      }
    ]
  ]
}
